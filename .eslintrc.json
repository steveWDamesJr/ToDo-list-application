{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "extends": ["airbnb-base"],
  "rules": {
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off",
    "import/extensions": [ 1, {
      "js": "always", "json": "always"
    }],
    "import/no-unresolved": [
      2, 
      { "caseSensitive": false }
   ]
  },
  "ignorePatterns": [
    "dist/",
    "build/"
  ]
}

"rules": {
  "import/no-unresolved": [
     2, 
     { "caseSensitive": false }
  ]
}
